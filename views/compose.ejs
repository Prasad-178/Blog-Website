<%- include('partials/header') -%> 

<h1>Compose</h1>
<form action="/compose" method="post">
    <h4><b>Title</b></h4>
    <input type="text" name="postTitle" id="postTitle" autocomplete="off" autofocus>
    <h4><b>Post</b></h4>
    <textarea cols="120" rows="5" type="text" name="postBody" id="postBody" autocomplete="off" ></textarea> <br> <br>
    <button type="submit" name="button">Publish</button>
</form>

<%- include('partials/footer') -%>